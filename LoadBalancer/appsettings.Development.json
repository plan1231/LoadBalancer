{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoadBalancer": {
    "Endpoints": [ "http://192.168.2.201:8080", "http://192.168.2.201:8081" ],
    "HealthCheck": {
      "Enabled": true,
      "Interval": 30,
      "Timeout": 5
    },
    "SessionAffinity": {
      "Enabled": true,
      "Policy": "SimpleCookie",
      "AffinityKeyName": "AffinityKey"
    },
    "Policy": "RoundRobin",
  }
}

